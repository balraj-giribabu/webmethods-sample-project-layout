pipeline{
	agent any
	environment{
		random_packages = "${PACKAGE-LIST}"
	}
	
	stages{
		stage("Creating Loop"){
			steps{
				script{
					packages_names = random_packages.split(',')
					for (pack in packages_names){
						println pack
						bat """
						dir
						dir
						"""
					}
				}
			}
		}
	}
}
